<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lib.bookbrain.dao.EnumMapper">
	<select id="getEnumsByGroup" resultType="com.lib.bookbrain.model.entity.MyEnum">
		select *
		from enum e
		where e.`group` = #{group};
	</select>
</mapper>